{
	"tokenColors": [
		{
			"settings": {
				"foreground": "#DCDCDC"
			}
		},
		{
			"name": "Comment",
			"scope": [
				"comment",
				"string.quoted.double.block.python"
			],
			"settings": {
				"foreground": "#3CB371"
			}
		},
		{
			"name": "Foreground, Operator",
			"scope": [
				"keyword.operator.class",
				"keyword.operator",
				"constant.other",
				"source.php.embedded.line"
			],
			"settings": {
				"foreground": "#DCDCDC"
			}
		},
		{
			"name": "Variable, String Link, Regular Expression, Tag Name",
			"scope": [
				"variable",
				"support.other.variable",
				"string.other.link",
				"string.regexp",
				"entity.name.tag",
				"entity.other.attribute-name",
				"meta.tag",
				"declaration.tag"
			],
			"settings": {
				"foreground": "#DD0000"
			}
		},
		{
			"name": "Number, Constant, Function Argument, Tag Attribute, Embedded",
			"scope": [
				"constant.numeric",
				"constant.language",
				"support.constant",
				"constant.character",
				"variable.parameter",
				"punctuation.section.embedded",
				"keyword.other.unit"
			],
			"settings": {
				"foreground": "#FFC58F"
			}
		},
		{
			"name": "Class, Support",
			"scope": [
				"entity.name.class",
				"entity.name.type.class",
				"support.type",
				"support.class"
			],
			"settings": {
				"fontStyle": "",
				"foreground": "#c0c0c0"
			}
		},
		{
			"name": "String, Symbols, Inherited Class, Markup Heading",
			"scope": [
				"string",
				"constant.other.symbol",
				"entity.other.inherited-class",
				"markup.heading"
			],
			"settings": {
				"foreground": "#D1F1A9"
			}
		},
		{
			"name": "Operator, Misc",
			"scope": [
				"keyword.operator",
				"constant.other.color"
			],
			"settings": {
				"foreground": "#99FFFF"
			}
		},
		{
			"name": "Function, Special Method, Block Level",
			"scope": [
				"entity.name.function",
				"meta.function-call",
				"support.function",
				"keyword.other.special-method",
				"meta.block-level"
			],
			"settings": {
				"foreground": "#4169e1"
			}
		},
		{
			"name": "Keyword, Storage",
			"scope": [
				"keyword",
				"storage",
				"storage.type",
				"entity.name.tag.css"
			],
			"settings": {
				"foreground": "#EBBBFF"
			}
		},
		{
			"name": "Invalid",
			"scope": "invalid",
			"settings": {
				"foreground": "#DCDCDC"
			}
		},
		{
			"name": "Separator",
			"scope": "meta.separator",
			"settings": {
				"foreground": "#DCDCDC"
			}
		},
		{
			"name": "Deprecated",
			"scope": "invalid.deprecated",
			"settings": {
				"fontStyle": "",
				"foreground": "#DCDCDC"
			}
		},
		{
			"name": "Diff foreground",
			"scope": [
				"markup.inserted.diff",
				"markup.deleted.diff",
				"meta.diff.header.to-file",
				"meta.diff.header.from-file"
			],
			"settings": {
				"foreground": "#DCDCDC"
			}
		},
		{
			"name": "Diff insertion",
			"scope": [
				"markup.inserted.diff",
				"meta.diff.header.to-file"
			],
			"settings": {
				"foreground": "#718c00"
			}
		},
		{
			"name": "Diff deletion",
			"scope": [
				"markup.deleted.diff",
				"meta.diff.header.from-file"
			],
			"settings": {
				"foreground": "#c82829"
			}
		},
		{
			"name": "Diff header",
			"scope": [
				"meta.diff.header.from-file",
				"meta.diff.header.to-file"
			],
			"settings": {
				"foreground": "#DCDCDC",
			}
		},
		{
			"name": "Diff range",
			"scope": "meta.diff.range",
			"settings": {
				"foreground": "#3e999f"
			}
		},
		{
			"name": "diff.deleted",
			"scope": "markup.deleted",
			"settings": {
				"foreground": "#F92672"
			}
		},
		{
			"name": "diff.inserted",
			"scope": "markup.inserted",
			"settings": {
				"foreground": "#A6E22E"
			}
		},
		{
			"name": "diff.changed",
			"scope": "markup.changed",
			"settings": {
				"foreground": "#967EFB"
			}
		}
	],
